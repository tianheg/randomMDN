{
  "manifest_version": 3,
  "name": "randomMDN",
  "version": "1.0.0",
  "description": "Open random MDN sites",
  "browser_specific_settings": {
    "gecko": {
      "id": "randommdn@tianhegao.com"
    }
  },
  "icons": {
    "48": "logo.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "randomNewTab.js"
    ]
  }
}